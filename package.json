{
  "name": "ra-men",
  "version": "1.0.0",
  "description": "a PWA AMP playground by react",
  "main": "src/index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "dev": "NODE_ENV=development webpack-dev-server  --inline --progress --colors --port 3000 --host 0.0.0.0 ",
    "test": "jest --colors",
    "coverage": "jest --colors --coverage"
  },
  "keywords": [
    "react",
    "pwa",
    "amp",
    "jest",
    "enzyme"
  ],
  "author": {
    "name": "hydraslay",
    "email": "xvkai2010@gmail.com"
  },
  "license": "MIT",
  "jest": {
    "unmockedModulePathPatterns": [
      "react",
      "enzyme",
      "jest-enzyme"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      ".*\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "ejs": "^2.5.7",
    "express": "^4.16.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-universal-component": "^2.5.5",
    "redux": "^3.7.2",
    "redux-logger": "^2.6.1",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.1.0",
    "styled-components": "^3.1.6",
    "webpack-flush-chunks": "^1.1.23",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^23.0.0-alpha.0",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-standalone": "^6.26.0",
    "copy-webpack-plugin": "^4.2.1",
    "cross-env": "^5.0.5",
    "css-hot-loader": "^1.3.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-15": "^1.0.5",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-react": "^7.7.0",
    "extract-css-chunks-webpack-plugin": "^2.0.17",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^3.0.7",
    "jest": "^22.4.2",
    "jest-enzyme": "^6.0.0",
    "react-test-renderer": "^16.2.0",
    "stats-webpack-plugin": "^0.6.1",
    "style-loader": "^0.18.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "webpack": "^4.1.1",
    "webpack-dev-server": "^3.1.1"
  }
}
